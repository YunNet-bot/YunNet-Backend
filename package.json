  "scripts": {
    "tsc": "tsc",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:migration:run": "typeorm migration:run",
    "typeorm:migration:revert": "typeorm migration:revert",
    "clean": "sh ./scripts/clean.sh",
    "test": "yarn clean && yarn build:swagger && yarn test:quick",
    "test:quick": "ts-mocha --paths 'test/**/*.ts' --timeout 10000",
    "test:watch": "yarn test -- --watchAll",
    "test:codecov": "yarn test -- --coverage",
    "build": "sh ./scripts/build.sh",
    "build:swagger": "sh ./scripts/build.sh swagger",
    "serve": "yarn run build && node dist/server.js",
    "serve:swagger": "yarn run build:swagger && node dist/server.js",
    "serve:quick": "node dist/server.js"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.0.4",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "mocha": "^8.2.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.5",
    "tsoa": "^3.5.0",
    "typeorm": "^0.2.29"
  },
  "_moduleAliases": {
    "@": "dist"
  }
