{
  "name": "yunnet_backend",
  "version": "3.0.0",
  "main": "index.js",
  "contributors": [
    "Clooooode <jackey8616@gmail.com> (https://www.clo5de.info)",
    "YoMin Su <aaaa1234567891016@gmail.com>",
    "TestEgg <acgseer@gmail.com>"
  ],
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:script": "ts-node scripts/typeorm.ts",
    "typeorm:migration:run": "yarn typeorm:script --migrations=run",
    "typeorm:migration:revert": "yarn typeorm:script --migrations=revert",
    "clean": "sh ./scripts/clean.sh",
    "test:unit": "ts-mocha --paths 'test/unit/**/*.ts' --timeout 10000",
    "test:unit:coverage": "nyc --report-dir coverage/unit yarn test:unit",
    "test:integration": "ts-mocha --require 'test/integration/hook.spec.ts' --paths 'test/integration/**/*.ts' --timeout 10000",
    "test:integration:coverage": "nyc --report-dir coverage/integration yarn test:integration",
    "test:coverage": "yarn test:unit:coverage && yarn test:integration:coverage",
    "test": "yarn test:unit && yarn test:integration",
    "build": "sh ./scripts/build.sh",
    "build:swagger": "sh ./scripts/build.sh swagger",
    "serve": "yarn run build && node dist/server.js",
    "serve:swagger": "yarn run build:swagger && node dist/server.js",
    "serve:quick": "node dist/server.js"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.16",
    "@types/lodash": "^4.14.191",
    "@types/minimist": "^1.2.1",
    "@types/mocha": "^8.0.4",
    "@types/node": "^18.11.18",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "minimist": "^1.2.5",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@yunology/ts-multi-tenancy": "^0.0.9",
    "cors": "^2.8.5",
    "express": "4.18.2",
    "lodash": "4.17.21",
    "module-alias": "^2.2.2",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.5",
    "tsoa": "^3.5.0",
    "typeorm": "0.3.6"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
